    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                " "
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                {
                    "command": "whichkey.show",
                    "args": [
                        {
                            "key": "m",
                            "name": "make...",
                            "type": "bindings",
                            "bindings": [
                                {
                                    "key": "m",
                                    "name": "Run drake::r_make()",
                                    "type": "command",
                                    "command": "r.runCommand",
                                    "args": "drake::r_make()"
                                },
                                {
                                    "key": "r",
                                    "name": "loadd drake targert at cursor",
                                    "type": "command",
                                    "command": "r.runCommandWithSelectionOrWord",
                                    "args": "drake::loadd($$)"
                                },
                                {
                                    "key": "p",
                                    "name": "source .packages.R",
                                    "type": "command",
                                    "command": "r.runCommand",
                                    "args": "source('./packages.R')"
                                }
                            ]
                        },
                        {
                            "key": "h",
                            "name": "help...",
                            "type": "bindings",
                            "bindings": [
                                {
                                    "key": "o",
                                    "name": "help on object at cursor",
                                    "type": "command",
                                    "command": "r.runCommandWithSelectionOrWord",
                                    "args": "help($$)"
                                }
                            ]
                        },
                        {
                            "key": "o",
                            "name": "output object",
                            "type": "command",
                            "command": "r.runCommandWithSelectionOrWord",
                            "args": "$$"
                        },
                        {
                            "key": "c",
                            "name": "code...",
                            "type": "bindings",
                            "bindings": [
                                {
                                    "key": "s",
                                    "name": "style code file",
                                    "type": "command",
                                    "command": "editor.action.formatDocument"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
